
function int factorial(int x){
	int aux;
	printl("La x: ");
	printl(x);
	if(x<=1){
		aux=1;
		printl("Caso base!!");
	}
	else{
		printl("LLAMADA!");
		aux=x*factorial(x-1);
	}
	return aux;
}

function void recursiva(int x){
	if(x<=1){
		tetragon(1,1,0);
	}
	else{
		recursiva(x-5);
		tetragon(x,x,0);
	}
}

