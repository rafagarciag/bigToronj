drawing(500,500,#FFFFFF){
	trans(250,250);
	push();
	int i=0,j=1;
	while(i<4){
	
		while(j<=5){
			trans(0,25);
			tetragon(j*5,j*5,1);
			trans(0,25);
			j=j+1;
		}
		pop();
		rotate(90);
		push();
		j=1;
		i=i+1;
	}
	
	rotate(45);
	push();
	set_color(#FF0000);
	for(i=0;i<4;i=i+1){
		for(j=1;j<=5;j=j+1){
			trans(0,25);
			tetragon(j*5,j*5,1);
			trans(0,25);
		}
		pop();
		rotate(90);
		push();
	}

}
